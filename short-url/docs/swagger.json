    "host": "petstore.swagger.io",
    "basePath": "/v2",
    "paths": {
        "/community/getCommunityList": {
            "get": {
                "description": "切換社區, 選擇社區用",
                "tags": [
                    "community"
                ],
                "summary": "獲取用戶擁有的社區列表",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用戶 firebase id",
                        "name": "userId",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "社區 id, 用來篩選特定社區, 測權限用的, 無權限回傳空陣列",
                        "name": "communityId",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}